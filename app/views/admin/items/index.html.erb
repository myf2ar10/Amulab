<section class="wrapper flex">

  <div class="admin_item_index">

  <h4>作品一覧</h4>

  <div class="items">
    <% @items.each do |item| %>
      <%= link_to admin_item_path(item) do %>
        <h5><%= item.name %></h5>
        <p><%= item.created_at.strftime("%Y/%m/%d %H:%M:%S") %></p>
      <% end %>
    <% end %>
  </div>

</section>

