<section class="wrapper flex">

  <div class="genre_index">

  <h4>ジャンル一覧</h4>

  <div class="genres">

    <table>
      <% @genres.each do |genre| %>
        <tr>
          <td>
            <h5><%= genre.name %></h5>
          </td>
        </tr>
        <tr>
          <td>
            <p><%= genre.created_at.strftime("%Y/%m/%d %H:%M:%S") %></p>
          </td>
        </tr>
      <% end %>
    </table>
    <%= paginate @genres %>

  </div>

</section>

